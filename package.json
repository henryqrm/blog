{
  "name": "blog",
  "version": "0.0.1",
  "author": "HenryQRM",
  "license": "MIT",
  "homepage": "https://henryqrm.github.io/blog",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run clean && npm run lint",
    "build": "react-scripts build && ts-node -O {\\\"module\\\":\\\"commonjs\\\"} node_modules/navi-scripts/bin/navi-scripts.js build",
    "pretest": "npm run lint",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "navi-scripts serve",
    "lint": "tslint \"src/**/*.ts*\"",
    "format": "prettier-tslint fix \"src/**/*.{tsx,ts}\"",
    "clean": "rm -f -r -d dist",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "blueimp-md5": "2.3.0",
    "feed": "2.0.2",
    "gh-pages": "^2.0.1",
    "import-all.macro": "2.0.3",
    "lint-staged": "^8.1.5",
    "lodash": "4.17.11",
    "navi": "^0.11.4",
    "navi-scripts": "^0.11.3",
    "netlify-cli": "2.6.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "prismjs": "1.15.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-navi": "^0.11.4",
    "react-scripts-mdx": "2.1.4",
    "rimraf": "2.6.2",
    "slugify": "1.3.2",
    "ts-node": "7.0.1",
    "tslint": "^5.13.1",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "3.2.2"
  },
  "jest": {
    "transform": {
      "\\.(jsx?|tsx?)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.ts*": [
      "prettier-tslint fix",
      "prettier-tslint check",
      "tslint .",
      "git add"
    ],
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
